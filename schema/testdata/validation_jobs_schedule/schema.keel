job NoArgs {
    @schedule
}

job TooManyArgs {
    @schedule(
        "every 10 minutes",
        "also mondays"
    )
}

job WrongArgType {
    @schedule(1234)
}

job Labelled {
    @schedule(cron: "foo")
}

job InvalidSchedule {
    @schedule("every 10 cats")
}

job InvalidCron {
    @schedule("*/10 * * * BOB")
}

job TwoSchedules {
    @schedule("every 10 minutes")
    @schedule("every 2 hours")
}
