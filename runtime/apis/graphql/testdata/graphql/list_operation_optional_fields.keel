model Person {
    fields {
        name Text?
        dateOfBirth Date?
        occupation Occupation?
    }

    operations {
        get getPerson(id)
        list listPeople(name, dateOfBirth, occupation)
        list listPeopleAllOptional(name?, dateOfBirth?, occupation?)
    }
}

enum Occupation {
    Doctor
    Teacher
    Astronaut
}

api Test {
    models {
        Person
    }
}
