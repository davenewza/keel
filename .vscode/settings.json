{
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--config=${workspaceFolder}/.golangci.yml",
        "--fast",
    ],
    "go.lintOnSave": "workspace",
    "protoc": {
        "options": [
            "--proto_path=${workspaceRoot}/tools/proto",
            "--proto_path=${workspaceRoot}/proto",
        ]
    }
}
