model Book {
    fields {
        author Author
    }
    
    actions {
        create createBooks() with (author)
        update updateBooks(id) with (author)
        list listBooks(author)
    }

    actions {
        create createBooksFunction() with (author)
        update updateBooksFunction(id) with (author)
        list listBooksFunction(author)
    }
}

model Author {
}
