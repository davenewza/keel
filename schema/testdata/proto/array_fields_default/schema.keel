model Thing {
    fields {
        texts Text[] @default(["science"])
        numbers Number[] @default([123,456])
        enums MyEnum[] @default([MyEnum.One, MyEnum.Two])

        emptyTexts Text[] @default([])
        emptyNumbers Number[] @default([])
        emptyEnums MyEnum[] @default([])
    }

    actions {
        create createThing()
    }
}

enum MyEnum {
    One
    Two
}