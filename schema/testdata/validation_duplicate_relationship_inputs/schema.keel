model Post {
    fields {
        name Text?
        blog Blog?
    }

    operations {
        create create() with (name, blog.name)
        create createDups() with (blog.name, blog.name)

        list list(name, blog.name)
        list listDups(blog.name, blog.name)
    }

}

model  Blog {
    fields {
        name Text?
    }
}
