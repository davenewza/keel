model Author {
    actions {
        get getAuthor(id)
        delete deleteAuthor(id)
    }
}

model Book {
	 actions {
        list searchBooks()
        delete deleteBook(id)
		    write deleteAll(Any) returns (Any)
    }
}

api Admin {
	models {
		Author
		Book {
			actions {
				deleteBook
				deleteAll
			}
		}
	}
}
api Api {
	models {
		Book {
			actions {
				searchBooks
			}
		}
	}
}