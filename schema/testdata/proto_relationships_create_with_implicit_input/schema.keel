model ChildModel {
    fields {
        isActive Boolean
        parent ParentModel
        parentOptional ParentModel?
    }

    operations {
        create createChild1() with (isActive, parent.id)
        create createChild2() with (isActive, parent.id, parentOptional.id)
    }
}

model ParentModel {
    fields {
        isActive Boolean
    }
}
