message PersonResponse {
    name Text
}

message PersonInput {
    name Text
    nested NestedObject
    optionalNested NestedObject?
}

message NestedObject {
    bool Boolean
}

model Person {
    fields {
        name Text
    }

    functions {
        read testAction(PersonInput) returns (PersonResponse)
    }
}

api Test {
    models {
        Person
    }
}
