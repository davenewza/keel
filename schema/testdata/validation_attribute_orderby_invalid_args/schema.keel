model Publisher { }

model Author {
    fields {
        firstName Text
        surname Text
        publisher Publisher
    }

    operations {
        list listAuthors() {
            @orderBy
            @orderBy(firstName)
            @orderBy(firstName: asc, nope: asc)
            @orderBy(firstName: asc, publisher: asc)
            @orderBy(firstName: asc, firstName: asc)
            @orderBy(firstName: "asc")
            @orderBy(firstName: 123)
            @orderBy(firstName: ASC)
        }
    }
}