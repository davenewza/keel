model User {
    fields {
        identity Identity @unique
    }
}
model BankAccount {
    fields {
        identity Identity @unique
    }
    actions {
        get getBankAccount(id) {
            @permission(expression: ctx.identity.user in bankAccount.identity.user)
            @permission(expression: ctx.identity.user > bankAccount.identity.user)
        }
    }
}